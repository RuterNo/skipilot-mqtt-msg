{
  "$id": "https://schemas.ruter.no/tp/api/skipilot/mqtt-msg/v1.0/vehicle-schedule/vehicle-schedule.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "VehicleSchedule",
  "type": "object",
  "required": [
    "id",
    "activities",
    "status",
    "datetime",
    "traceId"
  ],
  "description": "schedule is vehicles plan for the day",
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "description": "Stable alphanumeric reference that identifies vehicle schedule"
    },
    "activities": {
      "$id": "#/properties/activities",
      "type": "array",
      "items": {
        "type": "VehicleActivity"
      },
      "description": "List of vehicle activities"
    },
    "status": {
      "$id": "#/properties/status",
      "type": "string",
      "description": "Status of the schedule"
    },
    "datetime": {
      "$id": "#/properties/datetime",
      "type": "string",
      "description": "ISO 8601. Reflects the current UTC time."
    },
    "traceId": {
      "$id": "#/properties/traceId",
      "type": "string",
      "description": "Alphanumeric reference generated by the publisher of the event"
    }
  },
  "examples": [
    {
      "id": "schedule12334343",
      "status": "PENDING",
      "datetime": "2021-05-19T12:45:50Z",
      "traceId": "addf-df123456",
      "activities": [{
        "id": "mission12334343",
        "type": "mission",
        "status": "PENDING",
        "datetime": "2021-05-19T12:45:50Z",
        "traceId": "addf-df123456",
        "usageType": "stopCompletely",
        "stopId": "stop1"
      },{
        "id": "pickup12334343",
        "type": "pickup",
        "status": "PENDING",
        "datetime": "2021-05-19T12:45:50Z",
        "traceId": "addf-df123456",
        "bookingRef": "booking-1233"
      }
      ]
    }
  ]
}