{
  "$id": "https://schemas.ruter.no/tp/api/skipilot/mqtt-msg/v1.0/vehicle-schedule/pickup-status.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PickupStatus",
  "type": "object",
  "required": [
    "status",
    "traceId",
    "datetime"
  ],
  "description": "Pickup is bound to the passenger",
  "additionalProperties": false,
  "properties": {
    "status": {
      "$id": "#/properties/status",
      "type": "string",
      "description": "Status of the pickup, [PLANNED, NOSHOW, COMPLETED]"
    },
    "traceId": {
      "$id": "#/properties/traceId",
      "type": "string",
      "description": "Alphanumeric reference generated by the publisher of the event"
    },
    "datetime": {
      "$id": "#/properties/datetime",
      "type": "string",
      "description": "ISO 8601. Reflects the current UTC time."
    }
  },
  "examples": [
    {
      "status": "PLANNED",
      "traceId": "udd-121212",
      "datetime": "2021-05-19T12:45:50Z"
    }
  ]
}