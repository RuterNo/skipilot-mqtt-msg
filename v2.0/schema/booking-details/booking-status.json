{
  "$id": "https://schemas.ruter.no/tp/skipilot/api/v2.0/booking-details/booking-status.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "BookingStatus",
  "type": "object",
  "required": [
    "status",
    "traceId",
    "datetime"
  ],
  "description": "Booking status changes",
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/status",
      "type": "string",
      "description": "Status of the booking"
    },
    "datetime": {
      "$id": "#/properties/datetime",
      "type": "string",
      "description": "ISO 8601. Reflects the current UTC time."
    },
    "traceId": {
      "$id": "#/properties/traceId",
      "type": "string",
      "description": "Alphanumeric reference generated by the publisher of the event"
    }
  },
  "examples": [
    {
      "status": "IN_PROGRESS",
      "datetime": "2021-05-19T12:45:50Z",
      "traceId": "addf-df123456"
    }
  ]
}