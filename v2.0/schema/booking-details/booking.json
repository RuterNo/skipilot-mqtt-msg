{
  "$id": "https://schemas.ruter.no/tp/skipilot/api/v2.0/booking-details/booking.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Booking",
  "type": "object",
  "required": [
    "id",
    "name",
    "from",
    "to",
    "status",
    "traceId",
    "datetime"
  ],
  "description": "Booking is related to traveller",
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "description": "Stable alphanumeric reference that is unique"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "description": "name/reference of the traveller"
    },
    "from": {
      "$id": "#/properties/from",
      "type": "string",
      "description": "Destination traveller is to be picked up at"
    },
    "status": {
      "$id": "#/properties/status",
      "type": "string",
      "description": "Status of the booking"
    },
    "datetime": {
      "$id": "#/properties/datetime",
      "type": "string",
      "description": "ISO 8601. Reflects the current UTC time."
    },
    "traceId": {
      "$id": "#/properties/traceId",
      "type": "string",
      "description": "Alphanumeric reference generated by the publisher of the event"
    }
  },
  "examples": [
    {
      "id": "booking1234",
      "name": "Space Traveller",
      "from": "oslo gata",
      "to": "Slottsgata",
      "status": "ACTIVE",
      "datetime": "2021-05-19T12:45:50Z",
      "traceId": "addf-df123456"
    }
  ]
}