        "addressName":  "",
        "onStreet":  "",
        "streetNo":  "",
        "unit":  "",
        "zipCode":  "",
        "city":  "",
        "fullAddress":  "",
        "bookingComments": null,
        "bookingLegComments":  null,
        "bookingMobilityAids":  null,
        "bookingLegMobilityAids":  null,
        "fareToCollect":  null,
        "spaceType": null
      },{
        "id":  6,
