{
  "$id": "https://schemas.ruter.no/tp/api/skipilot/mqtt-msg/v2.0/vehicle-schedule/vehicle-schedule.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "VehicleSchedule",
  "type": "object",
  "required": [
    "id",
    "runId",
    "datetime",
    "traceId",
    "events"
  ],
  "description": "schedule is vehicles plan for the day",
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "number",
      "description": "Schedule id"
    },
    "runId": {
      "$id": "#/properties/runId",
      "type": "number",
      "description": "Driving run id"
    },
    "datetime": {
      "$id": "#/properties/datetime",
      "type": "string",
      "description": "ISO 8601. Reflects the current UTC time."
    },
    "traceId": {
      "$id": "#/properties/traceId",
      "type": "string",
      "description": "Alphanumeric reference generated by the publisher of the event"
    },
    "events": {
      "$id": "#/properties/events",
      "type": "array",
      "items": {
        "allOf": [{ "$ref": "https://raw.githubusercontent.com/RuterNo/skipilot-mqtt-msg/master/v2.0/schema/vehicle-schedule/event.json" }]
        }
      },
      "description": "List of events"
    }
  },
  "examples": [
    {
      "id": 6139,
      "runId": 1,
      "datetime": "2021-05-19T12:45:50Z",
      "traceId": "addf-df123456",
      "events": [{
        "id":  1,
        "name":  "ACT_PULLOUT",
        "bookingId": null,
        "lat":  null,
        "lon":  null,
        "estTime": null,
        "scheduledTime":  "11:16",
        "status":  1,
