{
  "$id": "https://schemas.ruter.no/tp/api/skipilot/mqtt-msg/v2.0/vehicle-schedule/vehicle-schedule.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "VehicleSchedule",
  "type": "object",
  "required": [
    "id",
    "runId",
    "datetime",
    "traceId",
    "events"
  ],
  "description": "schedule is vehicles plan for the day",
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "number",
      "description": "Schedule id"
    },
    "runId": {
      "$id": "#/properties/runId",
      "type": "number",
      "description": "Driving run id"
    },
    "datetime": {
      "$id": "#/properties/datetime",
      "type": "string",
      "description": "ISO 8601. Reflects the current UTC time."
    },
    "traceId": {
      "$id": "#/properties/traceId",
      "type": "string",
      "description": "Alphanumeric reference generated by the publisher of the event"
    },
    "events": {
      "$id": "#/properties/events",
      "type": "array",
      "items": {
        "$id": "https://schemas.ruter.no/tp/api/skipilot/mqtt-msg/v2.0/vehicle-schedule/event.json",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "title": "Event",
        "type": "object",
        "required": [
          "id",
          "name",
          "bookingId",
          "lat",
          "lon",
          "estTime",
          "scheduledTime",
          "status",
          "activity",
          "addressName",
          "onStreet",
          "streetNo",
          "unit",
          "zipCode",
          "city",
          "fullAddress",
          "bookingComments",
          "bookingLegComments",
          "bookingMobilityAids",
          "bookingLegMobilityAids",
          "fareToCollect",
          "spaceType"
        ],
        "description": "Event represents actions (act_pullout, act_pickup, act_dropoff, act_pullin) as defined in Trapeze booking system (Trapeze Wecore)",
        "additionalProperties": false,
        "properties": {
          "id": {
            "$id": "#/properties/id",
            "type": "number",
            "description": "Event id"
          },
          "name": {
            "$id": "#/properties/name",
            "type": "string",
            "description": "Event name, e.g. ACT_PULLOUT, ACT_PICKUP, etc"
          },
          "bookingId": {
            "$id": "#/properties/bookingId",
            "type": "number",
            "description": "Booking id, number if eventname is ACT_PICKUP or ACT_DROPOFF, null otherwise"
          },
          "lat": {
            "$id": "#/properties/lat",
            "type": "number",
            "description": "GPS coordinate latitude for possibly pickup/dropoff-station"
          },
          "lon": {
            "$id": "#/properties/lon",
            "type": "number",
            "description": "GPS coordinate longitude for possibly pickup/dropoff-station"
          },
          "estTime": {
            "$id": "#/properties/estTime",
            "type": "string",
            "description": "Estimated time for the trip"
          },
          "scheduledTime": {
            "$id": "#/properties/scheduledTime",
            "type": "string",
            "description": "Time (hour, min) trip is scheduled for: format: HH:mm"
          },
          "status": {
            "$id": "#/properties/status",
            "type": "integer",
            "description": "Single digit number expressing scheduled status for the event"
          },
          "activity": {
            "$id": "#/properties/activity",
            "type": "integer",
            "description": "Single digit number expressing event activity"
          },
          "addressName": {
            "$id": "#/properties/addressName",
            "type": "string",
            "description": "Possibly pickup address (stop address)"
          },
          "onStreet": {
            "$id": "#/properties/onStreet",
            "type": "string",
            "description": "Possibly pickup street (stop address)"
          },
          "unit": {
            "$id": "#/properties/unit",
            "type": "string",
            "description": "Unit"
          },
          "zipCode": {
            "$id": "#/properties/zipCode",
            "type": "string",
            "description": "Possibly pickup address zipcode (stop address)"
          },
          "city": {
            "$id": "#/properties/city",
            "type": "string",
            "description": "Possibly pickup address city (stop address)"
          },
          "fullAddress": {
            "$id": "#/properties/fullAddress",
            "type": "string",
            "description": "Possibly pickup address(full) in one line"
          },
          "bookingComments": {
            "$id": "#/properties/bookingComments",
            "type": "string",
            "description": "Booking comments"
          },
          "bookingLegComments": {
            "$id": "#/properties/bookingLegComments",
            "type": "string",
            "description": "Booking leg comments"
          },
          "bookingMobilityAids": {
            "$id": "#/properties/bookingMobilityAids",
            "type": "string",
            "description": "Booking mobility aids"
          },
          "bookingLegMobilityAids": {
            "$id": "#/properties/bookingLegMobilityAids",
            "type": "string",
            "description": "Booking leg mobility aids"
          },
          "fareToCollect": {
            "$id": "#/properties/fareToCollect",
            "type": "string",
            "description": "Fare to collect"
          },
          "spaceType": {
            "$id": "#/properties/spaceType",
            "type": "string",
            "description": "Space type"
          }
        }
      },
      "description": "List of events"
    }
  },
  "examples": [
    {
      "id": 6139,
      "runId": 1,
      "datetime": "2021-05-19T12:45:50Z",
      "traceId": "addf-df123456",
      "events": [{
        "id":  1,
        "name":  "ACT_PULLOUT",
        "bookingId": null,
        "lat":  null,
        "lon":  null,
        "estTime": null,
        "scheduledTime":  "11:16",
        "status":  1,
        "activity":  4,
        "addressName":  "",
        "onStreet":  "",
        "streetNo":  "",
        "unit":  "",
        "zipCode":  "",
        "city":  "",
        "fullAddress":  "",
        "bookingComments": null,
        "bookingLegComments":  null,
        "bookingMobilityAids":  null,
        "bookingLegMobilityAids":  null,
        "fareToCollect":  null,
        "spaceType": null
      },{
        "id":  6,
        "name":  "ACT_DROPOFF",
        "bookingId": 43,
        "lat":  null,
        "lon":  null,
        "estTime": null,
        "scheduledTime":  "12:00",
        "status":  1,
        "activity":  1,
        "addressName":  "MOTORSENTERET",
        "onStreet":  "LØXAVEIEN",
        "streetNo":  "2",
        "unit":  "",
        "zipCode":  "1352",
        "city":  "RUD, BÆRUM",
        "fullAddress":  "MOTORSENTERET LØXAVEIEN 2, 1351 RUD, BÆRUM",
        "bookingComments": null,
        "bookingLegComments":  null,
        "bookingMobilityAids":  null,
        "bookingLegMobilityAids":  null,
        "fareToCollect":  null,
        "spaceType": null
      }
      ]
    }
  ]
}