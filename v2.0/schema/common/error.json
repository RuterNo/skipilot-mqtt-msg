{
  "$id": "https://schemas.ruter.no/tp/skipilot/api/v2.0/common/error.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Error",
  "type": "object",
  "required": [
    "id",
    "code",
    "message",
    "datetime",
    "traceId",
    "refEventTraceId"
  ],
  "description": "schedule is vehicles plan for the day",
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "description": "Unique alphanumeric id for the error event"
    },
    "code": {
      "$id": "#/properties/code",
      "type": "string",
      "description": "Error code"
    },
    "message": {
      "$id": "#/properties/message",
      "type": "string",
      "description": "Detailed error message"
    },
    "datetime": {
      "$id": "#/properties/datetime",
      "type": "string",
      "description": "Datetime of the error event"
    },
    "traceId": {
      "$id": "#/properties/traceId",
      "type": "string",
      "description": "Alphanumeric reference generated by the publisher of the event"
    },
    "refEventTraceId": {
      "$id": "#/properties/refEventTraceId",
      "type": "string",
      "description": "Traceid for the event that failed"
    }
  },
  "examples": [
    {
      "id": "error1",
      "code": "409",
      "message": "Illegal status transition, status can not be set to IN_PROGRESS from PENDING",
      "datetime": "2021-05-19T12:45:50Z",
      "traceId": "addf-df123456",
      "refEventTraceId": "afdf-df12333"
    }
  ]
}